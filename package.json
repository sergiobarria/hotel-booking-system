{
    "name": "the-wild-oasis",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "format": "prettier --write ./src/**/*.{ts,tsx,js,jsx}",
        "check": "tsc --noEmit",
        "db:start": "pnpx supabase start",
        "db:stop": "pnpx supabase stop",
        "db:stop-no-backup": "pnpx supabase stop --no-backup",
        "db:status": "pnpx supabase status",
        "db:push": "pnpx supabase db push",
        "generate-types": "pnpx supabase gen types typescript --local > ./src/types/schemas.ts"
    },
    "dependencies": {
        "@fontsource-variable/sono": "^5.0.17",
        "@fontsource/poppins": "^5.0.8",
        "@radix-ui/react-slot": "^1.0.2",
        "@supabase/supabase-js": "^2.38.5",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "lucide-react": "^0.292.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.20.0",
        "tailwind-merge": "^2.0.0",
        "tailwindcss-animate": "^1.0.7"
    },
    "devDependencies": {
        "@types/node": "^20.9.4",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.53.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "postcss": "^8.4.31",
        "prettier": "^3.1.0",
        "prettier-plugin-tailwindcss": "^0.5.7",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2",
        "vite": "^5.0.0"
    }
}
